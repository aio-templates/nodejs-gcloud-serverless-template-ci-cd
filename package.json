{
  "name": "nodejs-googlecloudfunctions",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node index.js",
    "test": "npm run test-unit",
    "test-unit": "cross-env NODE_ENV=test ./node_modules/.bin/mocha test/unit/*.test.js",
    "test-integration": "cross-env NODE_ENV=test ./node_modules/.bin/mocha test/integration/*.test.js",
    "lint-ci": "./node_modules/.bin/eslint ./config/**/*.js ./migrations/**/*.js ./models/**/*.js ./server/**/*.js ./test/**/*.js index.js",
    "prettier-ci": "prettier-check bin/**/*.js ./config/**/*.js ./migrations/**/*.js ./models/**/*.js ./server/**/*.js ./test/**/*.js index.js",
    "sql:migrate:undo": "npm run sql db:migrate:undo:all",
    "sql:migrate": "npm run sql db:migrate",
    "sql:seed:undo": "npm run sql db:seed:undo:all",
    "sql:seed": "npm run sql db:seed:all",
    "sql:s": "npm run sql:migrate:undo && npm run sql:migrate && npm run sql:seed",
    "sql": "./node_modules/sequelize-cli/lib/sequelize",
    "sqlize:s": "sequelize db:migrate:undo && db:migrate && db:seed:all"
  },
  "dependencies": {
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.2",
    "cookie-parser": "^1.4.3",
    "debug": "^3.1.0",
    "express": "^4.16.2",
    "express-bearer-token": "^2.1.1",
    "firebase-admin": "^5.11.0",
    "morgan": "^1.7.0",
    "mysql2": "^1.6.4",
    "pg": "^6.1.0",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.38.0",
    "sequelize-cli": "^4.1.1",
    "serve-favicon": "^2.5.0",
    "sqlite3": "^4.0.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.0.1",
    "babel-minify": "^0.5.0",
    "babel-polyfill": "^6.0.16",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-minify": "^0.5.0",
    "babel-preset-stage-2": "^6.24.1",
    "cross-env": "^3.2.4",
    "eslint": "^5.6.1",
    "eslint-plugin-prettier": "^2.6.2",
    "expect.js": "^0.3.1",
    "mocha": "^5.2.0",
    "prettier": "^1.14.2",
    "prettier-check": "^2.0.0",
    "supertest": "^3.3.0",
    "webpack": "^4.20.2",
    "webpack-bundle-analyzer": "^3.0.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9",
    "webpack-merge": "^4.1.4"
  }
}
